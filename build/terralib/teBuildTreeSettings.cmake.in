# Compiled modules include path.
set( TE_INCLUDE_DIRS @_TE_INCLUDE_DIRS@)

# Modules directory
set (T5_MODULEPATH @T5_MODULES_PATH@ )
set (TE_DEPENDENCIES_DIR "@TE_DEPENDENCIES_DIR@" CACHE PATH "Find the directory containing terralib5 3rd-party dependencies.")
list (APPEND CMAKE_MODULE_PATH "${T5_MODULEPATH}")

#TerraLib dependencies dir
if(NOT "${TE_DEPENDENCIES_DIR}" STREQUAL "")
  list (APPEND CMAKE_PREFIX_PATH "${TE_DEPENDENCIES_DIR}")
endif()

# Configuration files folder.
set (DEFAULT_LOGCONF_FILE @ROOT@/log/te-log.conf)

# Compiled modules
set (TE_LIBRARIES @_TE_LIBRARIES@)

# Include the dependencies. 
include ( ${TERRALIB_CMAKE_DIR}/teDepends.cmake )

set (DEPS @TE_3DS@)

include ( ${T5_MODULEPATH}/Deps_versions.cmake )	
include ( ${T5_MODULEPATH}/UtilFunctions.cmake )	

foreach(DEP ${DEPS})
  find_package(${DEP} ${_${DEP}_VERSION} REQUIRED)
endforeach()

list (FIND DEPS "Qt4" HAS_QT4)

if(HAS_QT4)
  include (${QT_USE_FILE})
endif()
