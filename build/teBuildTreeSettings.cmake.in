# Compiled modules include path.
set( TE_INCLUDE_DIRS @CMAKE_BINARY_DIR@;@TE_INCLUDE_DIRS@)

# Modules directory
set (TE_MODULEPATH @TE_MODULES_PATH@ )
set (TE_DEPENDENCIES_DIR "@TE_DEPENDENCIES_DIR@" CACHE PATH "Find the directory containing terralib5 3rd-party dependencies.")
list (APPEND CMAKE_MODULE_PATH "${TE_MODULEPATH}")

#TerraLib dependencies dir
if(NOT "${TE_DEPENDENCIES_DIR}" STREQUAL "")
  list (APPEND CMAKE_PREFIX_PATH "${TE_DEPENDENCIES_DIR}" "${TE_DEPENDENCIES_DIR}/gdal")
  set (CMAKE_FIND_ROOT_PATH "${TE_DEPENDENCIES_DIR}" "${TE_DEPENDENCIES_DIR}/gdal")
endif()

# Configuration files folder.
set (TE_DEFAULT_LOGCONF_FILE @ROOT@/log/te-log.conf)

# Include the dependencies. 
#include ( ${TE_CMAKE_DIR}/teDepends.cmake )

set (TE_BIN_DIR @TE_BIN_DIR@)
set (TE_SCHEMA_LOCATION @TE_SCHEMA_LOCATION@)
set (TE_LOGO @TE_LOGO@)
set (TE_JSON_FILES_LOCATION @TE_JSON_FILES_LOCATION@)
set (TE_SQL_FILES_LOCATION @TE_SQL_FILES_LOCATION@)
set (TE_XML_FILES_LOCATION @TE_XML_FILES_LOCATION@)

set (DEPS @TE_3DS@)

include ( ${TE_MODULEPATH}/Deps_versions.cmake )	
include ( ${TE_MODULEPATH}/UtilFunctions.cmake )	

foreach(DEP ${DEPS})
  find_package(${DEP} ${_${DEP}_VERSION} REQUIRED)
endforeach()
