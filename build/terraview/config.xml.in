<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<Application xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
             xmlns="http://www.terralib.org/schemas/af"
             xsd:schemaLocation="http://www.terralib.org/schemas/af ${TE_SCHEMA_LOCATION}/af/af.xsd"
             version="${TE_STRING_VERSION}"
             release="${TE_STRING_BUILD_DATE}">
  <Organization>INPE</Organization>
  <Name>TerraView</Name>	
  <Title>TerraView</Title>
  <IconName>${TVIEW_ICON}</IconName>	
  <UserSettingsFile xlink:href="${TVIEW_OUT_DIR}/user_settings.xml"/>
  <PluginsFile xlink:href="${TVIEW_OUT_DIR}/application_plugins.xml"/>
  <HelpFile xlink:href="${TVIEW_RESOURCE_FILE}"/>
  <IconThemeInfo>
    <BaseDirectory xlink:href="${TE_ICONS_THEME_PATH}"/>
    <DefaultTheme>${TE_ICONS_THEME}</DefaultTheme>
  </IconThemeInfo>
  <ToolBarDefaultIconSize>24</ToolBarDefaultIconSize>
  <DefaultPluginsCategory>
    <Category>Cellular Space</Category>
    <Category>Data Access</Category>
    <Category>Data Management</Category>
    <Category>Examples</Category>
    <Category>Language Bindings</Category>
    <Category>Location Base Services</Category>
    <Category>Plugin Managememt</Category>
    <Category>Spatial Analysis</Category>
    <Category>Spatial Operations</Category>
    <Category>Web Services</Category>
  </DefaultPluginsCategory>
  <AboutDialogLogo xlink:href="${TE_ABOUT_LOGO}"/>
  <TerraLibLogo xlink:href="${TE_LOGO}"/>
</Application>