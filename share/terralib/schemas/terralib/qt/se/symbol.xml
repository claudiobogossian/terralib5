<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!-- A simple example of TerraLib Symbol Library XML file -->
<SymbolLibrary xmlns="http://www.terralib.org/schemas/qt/se"
               xsd:schemaLocation="http://www.terralib.org/schemas/qt/se symbol.xsd"
               xmlns:se="http://www.opengis.net/se"
               xmlns:ogc="http://www.opengis.net/ogc"
               xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
               xmlns:xlink="http://www.w3.org/1999/xlink"
               name="TerraLib 5 Default Symbol Library"
               version="5.0.0">

  <Symbol>
    <Id>0</Id>
    <Name>Highway</Name>
    <Author>TerraLib Team</Author>
    <Tags>line; highway; road;</Tags>
    <Description>This symbol can be used to represent highways.</Description>
    <se:LineSymbolizer>
      <se:Stroke>
        <se:SvgParameter name="stroke"><ogc:Literal>#000000</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-width"><ogc:Literal>5</ogc:Literal></se:SvgParameter>
      </se:Stroke>
    </se:LineSymbolizer>
    <se:LineSymbolizer>
      <se:Stroke>
        <se:SvgParameter name="stroke"><ogc:Literal>#808080</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-width"><ogc:Literal>3</ogc:Literal></se:SvgParameter>
    </se:Stroke>
    </se:LineSymbolizer>
    <se:LineSymbolizer>
      <se:Stroke>
        <se:SvgParameter name="stroke"><ogc:Literal>#FFFF00</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-width"><ogc:Literal>1</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-dasharray"><ogc:Literal>2 2</ogc:Literal></se:SvgParameter>
      </se:Stroke>
    </se:LineSymbolizer>
  </Symbol>

  <Symbol>
    <Id>1</Id>
    <Name>Red</Name>
    <Author>TerraLib Team</Author>
    <Tags>red; polygon; fill; area;</Tags>
    <Description>A basic polygon symbol.</Description>
    <se:PolygonSymbolizer>
      <se:Fill>
        <se:SvgParameter name="fill"><ogc:Literal>#FF0000</ogc:Literal></se:SvgParameter>
      </se:Fill>
      <se:Stroke>
        <se:SvgParameter name="stroke"><ogc:Literal>#000000</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-width"><ogc:Literal>1</ogc:Literal></se:SvgParameter>
      </se:Stroke>
    </se:PolygonSymbolizer>
  </Symbol>

  <Symbol>
    <Id>2</Id>
    <Name>Star</Name>
    <Author>TerraLib Team</Author>
    <Tags>point; star; yellow</Tags>
    <Description>A basic point symbol.</Description>
    <se:PointSymbolizer>
      <se:Graphic>
        <se:Mark>
          <se:WellKnownName>star</se:WellKnownName>
          <se:Fill>
            <se:SvgParameter name="fill"><ogc:Literal>#FFFF00</ogc:Literal></se:SvgParameter>
          </se:Fill>
          <se:Stroke>
            <se:SvgParameter name="stroke"><ogc:Literal>#000000</ogc:Literal></se:SvgParameter>
            <se:SvgParameter name="stroke-width"><ogc:Literal>1</ogc:Literal></se:SvgParameter>
          </se:Stroke>
       </se:Mark>
       <se:Size><ogc:Literal>16</ogc:Literal></se:Size>
      </se:Graphic>
    </se:PointSymbolizer>
  </Symbol>
  
  <Symbol>
    <Id>19</Id>
    <Name>Simple Line</Name>
    <Author>TerraLib Team</Author>
    <Tags>line; simple</Tags>
    <Description>A basic line symbol.</Description>
    <se:LineSymbolizer>
      <se:Stroke>
        <se:SvgParameter name="stroke"><ogc:Literal>#000000</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-width"><ogc:Literal>1</ogc:Literal></se:SvgParameter>
      </se:Stroke>
	</se:LineSymbolizer>
  </Symbol>
  
  <Symbol>
    <Id>20</Id>
    <Name>Dot Line</Name>
    <Author>TerraLib Team</Author>
    <Tags>line; dot</Tags>
    <Description>A basic line symbol.</Description>
    <se:LineSymbolizer>
      <se:Stroke>
        <se:SvgParameter name="stroke"><ogc:Literal>#000000</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-width"><ogc:Literal>1</ogc:Literal></se:SvgParameter>
		<se:SvgParameter name="stroke-dasharray"><ogc:Literal>2 2</ogc:Literal></se:SvgParameter>
      </se:Stroke>
	</se:LineSymbolizer>
  </Symbol>
  
  <Symbol>
    <Id>21</Id>
    <Name>Dash Line</Name>
    <Author>TerraLib Team</Author>
    <Tags>line; dash</Tags>
    <Description>A basic line symbol.</Description>
    <se:LineSymbolizer>
      <se:Stroke>
        <se:SvgParameter name="stroke"><ogc:Literal>#000000</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-width"><ogc:Literal>1</ogc:Literal></se:SvgParameter>
		<se:SvgParameter name="stroke-dasharray"><ogc:Literal>5 5</ogc:Literal></se:SvgParameter>
      </se:Stroke>
	</se:LineSymbolizer>
  </Symbol>
  
  <Symbol>
    <Id>22</Id>
    <Name>Simple Line</Name>
    <Author>TerraLib Team</Author>
    <Tags>line; simple</Tags>
    <Description>A basic line symbol.</Description>
    <se:LineSymbolizer>
      <se:Stroke>
        <se:SvgParameter name="stroke"><ogc:Literal>#000000</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-width"><ogc:Literal>3</ogc:Literal></se:SvgParameter>
      </se:Stroke>
	</se:LineSymbolizer>
  </Symbol>
  
  <Symbol>
    <Id>23</Id>
    <Name>Simple Line</Name>
    <Author>TerraLib Team</Author>
    <Tags>line; simple</Tags>
    <Description>A basic line symbol.</Description>
    <se:LineSymbolizer>
      <se:Stroke>
        <se:SvgParameter name="stroke"><ogc:Literal>#000000</ogc:Literal></se:SvgParameter>
        <se:SvgParameter name="stroke-width"><ogc:Literal>5</ogc:Literal></se:SvgParameter>
      </se:Stroke>
	</se:LineSymbolizer>
  </Symbol>

</SymbolLibrary>
