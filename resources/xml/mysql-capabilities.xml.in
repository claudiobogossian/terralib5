<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<DataSourceCapabilities xmlns="http://www.terralib.org/schemas/dataaccess"
						xmlns:te_dt="http://www.terralib.org/schemas/datatype"
						xmlns:te_common="http://www.terralib.org/schemas/common"
						xmlns:te_gm="http://www.terralib.org/schemas/geometry"
						xmlns:xlink="http://www.w3.org/1999/xlink"
						xmlns:xsd="http://www.w3.org/2001/XMLSchema-instance"
						xsd:schemaLocation="http://www.terralib.org/schemas/dataaccess ${TE_SCHEMA_LOCATION}/dataaccess/capabilities.xsd"
						version="${TE_STRING_VERSION}">
  
  <DataTypeCapabilities>
    <DataType>INT16_TYPE</DataType>
    <DataType>INT32_TYPE</DataType>
    <DataType>INT64_TYPE</DataType>
    <DataType>DOUBLE_TYPE</DataType>
    <DataType>STRING_TYPE</DataType>
    <DataType>BYTE_ARRAY_TYPE</DataType>
    <DataType>GEOMETRY_TYPE</DataType>
    <DataType>DATETIME_TYPE</DataType>
    <DataType>RASTER_TYPE</DataType>
  </DataTypeCapabilities>
  <QueryCapabilities>
    <SQLDialect>true</SQLDialect>
    <Insert>true</Insert>
    <Update>true</Update>
    <Delete>true</Delete>
    <Create>true</Create>
    <Drop>true</Drop>
    <Alter>true</Alter>
    <Select>true</Select>
    <SelectInto>true</SelectInto>
    <SpatialOperators>
      <Function name="st_astext">
        <FunctionEncoder alias="AsText"/>
      </Function>
      <Function name="st_asbinary">
        <FunctionEncoder alias="AsBinary"/>
      </Function>
      <Function name="st_dimension">
        <FunctionEncoder alias="Dimension"/>
      </Function>
      <Function name="st_envelope">
        <FunctionEncoder alias="Envelope"/>
      </Function>
      <Function name="st_geometrytype">
        <FunctionEncoder alias="GeometryType"/>
      </Function>
      <Function name="st_srid">
        <FunctionEncoder alias="SRID"/>
      </Function>
      <Function name="st_boundary">
        <FunctionEncoder alias="Boundary"/>
      </Function>
      <Function name="st_isempty">
        <FunctionEncoder alias="IsEmpty"/>
      </Function>
      <Function name="st_issimple">
        <FunctionEncoder alias="IsSimple"/>
      </Function>
      <Function name="st_isring">
        <FunctionEncoder alias="IsRing"/>
      </Function>
      <Function name="st_x">
        <FunctionEncoder alias="X"/>
      </Function>
      <Function name="st_y">
        <FunctionEncoder alias="Y"/>
      </Function>
      <Function name="st_endpoint">
        <FunctionEncoder alias="EndPoint"/>
      </Function>
      <Function name="st_length">
        <FunctionEncoder alias="GLength"/>
      </Function>
      <Function name="st_numpoints">
        <FunctionEncoder alias="NumPoints"/>
      </Function>
      <Function name="st_pointn">
        <FunctionEncoder alias="PointN"/>
      </Function>
      <Function name="st_startpoint">
        <FunctionEncoder alias="StartPoint"/>
      </Function>
      <Function name="st_isclosed">
        <FunctionEncoder alias="IsClosed"/>
      </Function>
      <Function name="st_area">
        <FunctionEncoder alias="Area"/>
      </Function>
      <Function name="st_exteriorring">
        <FunctionEncoder alias="ExteriorRing"/>
      </Function>
      <Function name="st_centroid">
        <FunctionEncoder alias="Centroid"/>
      </Function>
      <Function name="st_interiorringn">
        <FunctionEncoder alias="InteriorRingN"/>
      </Function>
      <Function name="st_numinteriorrings">
        <FunctionEncoder alias="NumInteriorRings"/>
      </Function>
      <Function name="st_geometryn">
        <FunctionEncoder alias="GeometryN"/>
      </Function>
      <Function name="st_numgeometries">
        <FunctionEncoder alias="NumGeometries"/>
      </Function>
      <Function name="st_buffer">
        <FunctionEncoder alias="Buffer"/>
      </Function>
      <Function name="st_convexhull">
        <FunctionEncoder alias="ConvexHull"/>
      </Function>
      <Function name="st_difference">
        <FunctionEncoder alias="Difference"/>
      </Function>
      <Function name="st_intersection">
        <FunctionEncoder alias="Intersection"/>
      </Function>
      <Function name="st_symdifference">
        <FunctionEncoder alias="SymDifference"/>
      </Function>
      <Function name="st_union">
        <FunctionEncoder alias="Union"/>
      </Function>
      <Function name="st_contains">
        <FunctionEncoder alias="Contains"/>
      </Function>
      <Function name="st_crosses">
        <FunctionEncoder alias="Crosses"/>
      </Function>
      <Function name="st_disjoint">
        <FunctionEncoder alias="Disjoint"/>
      </Function>
      <Function name="st_equals">
        <FunctionEncoder alias="Equals"/>
      </Function>
      <Function name="st_intersects">
        <FunctionEncoder alias="Intersects"/>
      </Function>
      <Function name="st_overlaps">
        <FunctionEncoder alias="Overlaps"/>
      </Function>
      <Function name="st_touches">
        <FunctionEncoder alias="Touches"/>
      </Function>
      <Function name="st_within">
        <FunctionEncoder alias="Within"/>
      </Function>
    </SpatialOperators>    
    <LogicalOperators>
      <Function name="and">
        <BinaryOpEncoder alias="AND"/>
      </Function>
      <Function name="or">
        <BinaryOpEncoder alias="OR"/>
      </Function>
      <Function name="not">
        <BinaryOpEncoder alias="NOT"/>
      </Function>
    </LogicalOperators>
    <ComparsionOperators>
      <Function name="=">
        <BinaryOpEncoder alias="="/>
      </Function>
      <Function name="&#60;&#62;">
        <BinaryOpEncoder alias="&#60;&#62;"/>
      </Function>
      <Function name="&#62;">
        <BinaryOpEncoder alias="&#62;"/>
      </Function>
      <Function name="&#60;">
        <BinaryOpEncoder alias="&#60;"/>
      </Function>
      <Function name="&#62;=">
        <BinaryOpEncoder alias="&#62;="/>
      </Function>
      <Function name="&#60;=">
        <BinaryOpEncoder alias="&#60;="/>
      </Function>
    </ComparsionOperators>
    <ArithmeticOperators>
      <Function name="+">
        <BinaryOpEncoder alias="+"/>
      </Function>
      <Function name="-">
        <BinaryOpEncoder alias="-"/>
      </Function>
      <Function name="*">
        <BinaryOpEncoder alias="*"/>
      </Function>
      <Function name="/">
        <BinaryOpEncoder alias="/"/>
      </Function>
    </ArithmeticOperators>
    <Functions>
      <Function name="upper">
        <FunctionEncoder alias="upper"/>
      </Function>
      <Function name="lower">
        <FunctionEncoder alias="lower"/>
      </Function>
      <Function name="trim">
        <FunctionEncoder alias="trim"/>
      </Function>
      <Function name="substring">
        <TemplateEncoder alias="substring" template="($1 from $2 for $3)"/>
      </Function>
      <Function name="sum">
        <FunctionEncoder alias="SUM"/>
      </Function>
      <Function name="avg">
        <FunctionEncoder alias="AVG"/>
      </Function>
      <Function name="count">
        <FunctionEncoder alias="COUNT"/>
      </Function>
      <Function name="max">
        <FunctionEncoder alias="MAX"/>
      </Function>
      <Function name="min">
        <FunctionEncoder alias="MIN"/>
      </Function>
    </Functions>
    <GeomOperands>
      <GeomType>Point</GeomType>
      <GeomType>LineString</GeomType>
      <GeomType>Polygon</GeomType>
      <GeomType>MultiPoint</GeomType>
      <GeomType>MultiLineString</GeomType>
      <GeomType>MultiPolygon</GeomType>
      <GeomType>GeometryCollection</GeomType>
    </GeomOperands>
  </QueryCapabilities>
  <DataSetTypeCapabilities>
    <SupportPrimaryKey>true</SupportPrimaryKey>
    <SupportUniqueKey>true</SupportUniqueKey>
    <SupportForeignKey>true</SupportForeignKey>
    <SupportSequence>false</SupportSequence>
    <SupportCheckConstraints>true</SupportCheckConstraints>
    <SupportIndex>true</SupportIndex>
    <SupportRTreeIndex>true</SupportRTreeIndex>
    <SupportBTreeIndex>true</SupportBTreeIndex>
    <SupportHashIndex>false</SupportHashIndex>
    <SupportQuadTreeIndex>false</SupportQuadTreeIndex>
  </DataSetTypeCapabilities>
  <DataSetCapabilities>
    <Bidirectional>false</Bidirectional>
    <Random>false</Random>
    <Indexed>false</Indexed>
    <EfficientMovePrevious>false</EfficientMovePrevious>
    <EfficientMoveBeforeFirst>true</EfficientMoveBeforeFirst>
    <EfficientMoveLast>false</EfficientMoveLast>
    <EfficientMoveAfterLast>false</EfficientMoveAfterLast>
    <EfficientMove>false</EfficientMove>
    <EfficientSize>false</EfficientSize>
    <Insertion>false</Insertion>
    <Update>false</Update>
    <Deletion>false</Deletion>
  </DataSetCapabilities>
  <SpecificCapabilities>  
  </SpecificCapabilities>
  <AccessPolicyName>RW_ACCESS</AccessPolicyName>
  <SupportTransactions>true</SupportTransactions>
  <SupportDataSetPesistenceAPI>true</SupportDataSetPesistenceAPI>
  <SupportDataSetTypePesistenceAPI>true</SupportDataSetTypePesistenceAPI>
  <SupportPreparedQueryAPI>true</SupportPreparedQueryAPI>
  <SupportBatchExecutorAPI>true</SupportBatchExecutorAPI>
</DataSourceCapabilities>